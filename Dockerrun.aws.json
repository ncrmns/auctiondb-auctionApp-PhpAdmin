{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "essential": true,
            "image": "adminer",
            "name": "adminer",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        },
        {
            "essential": true,
            "image": "node:10",
            "name": "app",
            "portMappings": [
                {
                    "containerPort": 1234,
                    "hostPort": 1234
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "root"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "auctiondb"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "ncrmns"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "root"
                }
            ],
            "essential": true,
            "image": "mariadb",
            "name": "db",
            "portMappings": [
                {
                    "containerPort": 3306,
                    "hostPort": 3306
                }
            ]
        }
    ],
    "family": "",
    "volumes": []
}
