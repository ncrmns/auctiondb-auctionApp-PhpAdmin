{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "essential": true,
      "image": "adminer",
      "name": "adminer",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    },
    {
      "essential": true,
      "image": "ncrmns/auctionappdb",
      "name": "app",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 1234,
          "hostPort": 1234
        }
      ]
    },
    {
      "environment": [
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "root"
        },
        {
          "name": "MYSQL_DATABASE",
          "value": "auctiondb"
        },
        {
          "name": "MYSQL_USER",
          "value": "ncrmns"
        },
        {
          "name": "MYSQL_PASSWORD",
          "value": "root"
        }
      ],
      "essential": true,
      "image": "mariadb",
      "memory": 128,
      "mountPoints": [
        {
          "containerPath": "/var/lib/mysql",
          "sourceVolume": "Container-Volume"
        },
        {
          "containerPath": "/docker-entrypoint-initdb.d/dump.sql",
          "sourceVolume": "_AppDump_Sql"
        }
      ],
      "name": "db",
      "portMappings": [
        {
          "containerPort": 3306,
          "hostPort": 3306
        }
      ]
    }
  ],
  "family": "",
  "volumes": [
    {
      "host": {
        "sourcePath": "container-volume"
      },
      "name": "Container-Volume"
    },
    {
      "host": {
        "sourcePath": "./app/dump.sql"
      },
      "name": "_AppDump_Sql"
    }
  ]
}